     name : OnModifyFilter
     internal_properties : CAAAAAgAAAARq9M4ETa2eEVv9kjq45+XVTTUWgKwoHJqip+7UkmmSMbU1wZZLb/sY1N+5ABWe3w2rtoK75OgHZWrrI9nO6mPeG2yMQ36t2PhOZPWg67Nv8q5ffDpFoiQdXqKj84fDPbF12fnI763nzTrE6eyNITCOks4M6Tz+2WzzLX81w0iDnAQzXpkHmMWqUvU/X9nFou9jdFKhmzdYDDE5eX0K8Mc06bwKFAsbbdg+N+GaDJnjgl+xUL2
     procedure_id : 2194324599994061523
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      // OnModifyFilter ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure OnModifyFilter()
      
      // If a filtering string is entered
      if EDT_Filter<>"" then
      	// Filter the records whose last name, first name or company contains the filter
      	QRY_Registered.pFilter = EDT_Filter
      else
      	// Otherwise, no filter
      	QRY_Registered.pFilter = Null
      end
      
      // Locks the selection code when re-displaying the looper
      gbDoNotSelect = True
      
      // Re-displays the looper by running the query
      LooperDisplay(LOOP_REGISTRATION,taInit)
      LooperSelectMinus(LOOP_REGISTRATION)
      
      // Re-enables the selection code of the looper
      gbDoNotSelect = False
     type : 458752
