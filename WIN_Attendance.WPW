     code : |1-
      // Syntax: 
      //DisplayDetails()
      // Displays the personal details
      // Parameters:
      //	None
      // Return value:
      // 	None
      procedure DisplayDetails()
      
      sDisp is string
      sDisp = QRY_Registered.Company+[CR]+QRY_Registered.Title+" "+ QRY_Registered.LastNameFirstName
      
      STC_CR = sDisp
      
