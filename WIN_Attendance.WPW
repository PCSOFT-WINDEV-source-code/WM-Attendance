    name : BTN_Scan_a_QR_Code
    identifier : 0x2e73ce990a8d12d3
    internal_properties : 
    properties :
     x : 6
     y : 434
     width : 312
     height : 40
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAACfmzeIgA2mGnZbjeQU3pCKfBOgRpKUAp2pb+BLgRRIMq6XnhuZl+UZfczOTfD6Zekl2h6nIcrF5akXKNq2anjLAVebqjrzuDuh89NELovKjOamek9BIYyjUuFE9+snnd8DsaS300+5Mphu2wBj/ThaJZ9j3UI65S7xarAzI871mG6whJG33pJA4HzT4z63C1WgqV/c6KVAafxQWCxWRolzoRMjITO/KdGdZAtPzQ/Qv+sS5WFo6pWW
        code : |1+
         // Triggers the scan of a QR-Code
         bc is a BarCode
         bc = BCCapture(BC_QRCODE)
         
         // In simulator mode, the reading of QRCode being unavailable, we are going to simulate the reading of an address (once an explanation message was displayed)
         if IniOSSimulatorMode or InAndroidSimulatorMode then
         	Info("In simulator, the reading of QRCode is not available.","For the test, we simulate the retrieval of the email address.")
         	// In simulator mode, assign the content of the bar code manually
         	bc..ContentType = bcTypeEmail
         	bc..Content = "luscan.ovide@libertysurf.fr"
         else
         	if ErrorOccurred() then 
         		Error("An error occurred while reading the QRCode.","Details :",ErrorInfo())
         		return
         	end
         end
         
         // The content of the bar code is an email address
         if bc..ContentType = bcTypeEmail then
         	// This email address is found in the looper
         	nRowLOOP is int = LooperSeek(ATT_Email,bc..Content)
         	if nRowLOOP>0 then
         		// Selects the email address and displays the detailed window
         		LooperSelectPlus(LOOP_REGISTRATION,nRowLOOP)
         		SelectionRegistered()
         	end
         end
        type : 18
      -
        type : 161
        enabled : false
